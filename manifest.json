{
    "name": "NoAutoplay",
    "manifest_version": 2,
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "<all_urls>", "storage"]
}